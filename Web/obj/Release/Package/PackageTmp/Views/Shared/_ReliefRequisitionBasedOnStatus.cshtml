<script type="text/javascript">
    $(function () {
        //$.getJSON('@Url.Action("PieRequests", "Dashboard")', {}, function (data) {
        $.getJSON('/Dashboard/ReliefRequisitionBasedOnStatus', {}, function (data) {
             var json = data;

             for (var i in json) {
                 // var serie = new Array(json[i].Projects, json[i].Bugs);
                 var cnt = '<tr>';
                 cnt = '<td>' + json[i].RequisitionNo + '</td>';
                 cnt = '<td>' + json[i].RequestedDate + '</td>';
                 cnt = '<td>' + json[i].Status + '</td>';
                 cnt = '<td class="td-actions">';
                 cnt = '   <a href="javascript:;" class="btn btn-small btn-warning">';
                 cnt = '      <i class="btn-icon-only icon-view">View</i>';
                 cnt = '  </a>';
                 cnt = '</td>';
                 cnt = '</tr>';
             }
             $(".status").html(cnt);
             
         });
     });
</script>

<div class="chart-wrapper">
    <div class="chart-inner">
        <div id="container" style="width: 100%; height: 100%;"></div>
    </div>
</div>

<table class="table table-striped table-bordered">
    <thead>
        <tr>
            <th>@Html.DisplayNameFor(model => model.RequisitionNo)</th>
            <th>@Html.DisplayNameFor(model => model.BenficiaryNo)</th>
            <th>@Html.DisplayNameFor(model => model.Status)</th>
            <th class="td-actions">Action</th>
        </tr>
    </thead>
    <tbody>    
        <div class="status"></div>
    </tbody>
</table>
