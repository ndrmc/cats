@model IEnumerable<Cats.Models.Hubs.FieldChange>
@using Telerik.Web.Mvc.UI

@(Html.Telerik().Grid(Model).Name("grdAudits").Columns(col => {

    col.Bound(c => c.ChangeDate);
    col.Bound(c => c.PreviousValue);
    col.Bound(c => c.ChangedValue);
    col.Bound(c => c.ChangedBy);
}).Pageable()

)



