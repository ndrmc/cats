@using System.Collections
<ul class="nav nav-list">
    <li class="nav-header">@ViewData["RegionName"]</li>
    <li class="divider"></li>
    @foreach (var item in ViewBag.Zones)
    {
        int id = @item.AdminUnitID;
        <li><a href="#@item.Name" OnClicks="zoneChange(@id);">@item.Name </a></li>
    }
<!--
    <li class="divider"></li>
    <li><a href="#" onclick="zoneChange('');">Show All Zones </a></li>
-->
</ul>
