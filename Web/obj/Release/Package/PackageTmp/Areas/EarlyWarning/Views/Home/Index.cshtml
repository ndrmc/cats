                                                ben[index] = ben[index] + double.Parse(@String.Format("{0:n0}", @cell));
                                                index++;
                                                <td class="number">@String.Format("{0:n0}", @cell)</td>
                                            }
                                            else
                                            {
                                                <td>@String.Format("{0:n0}", @cell)</td>
                                            }
                                        }
                                    </tr>
                                }
                            </tbody>
                            <tr>
                                <th>Total:</th>
                                @foreach (double t in @ben)
                                {
                                    <th class="number">@String.Format("{0:n0}", @t) </th>
                                }
